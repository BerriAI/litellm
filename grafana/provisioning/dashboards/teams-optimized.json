    {
      "id": 6,
      "title": "📈 Динамика расходов по проектам",
      "type": "timeseries",
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 18},
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
