      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  date_trunc('hour', sl.\"startTime\") as time,\n  COALESCE(sl.metadata->>'user_api_key_team_alias', 'unknown') as metric,\n  SUM(sl.spend) as value\nFROM \"LiteLLM_SpendLogs\" sl\nWHERE\n  sl.\"startTime\" >= $__timeFrom()\n  AND sl.\"startTime\" < $__timeTo()\n  AND (sl.metadata->>'user_api_key_team_alias' ~ '$team_filter' OR '$team_filter' = '.*')\n  AND sl.metadata->>'user_api_key_team_alias' IS NOT NULL\n  AND sl.metadata->>'user_api_key_team_alias' != ''\nGROUP BY date_trunc('hour', sl.\"startTime\"), sl.metadata->>'user_api_key_team_alias'\nORDER BY time, metric"
        }
