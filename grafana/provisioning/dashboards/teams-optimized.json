        "definition": "label_values(litellm_spend_usd_total{team_alias!=\"\"}, team_alias)",
        "hide": 0,
        "sort": 1,
        "skipUrlSync": false
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "💰 Общие расходы проектов (Всего)",
      "type": "stat",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "SELECT\n  SUM(sl.spend) as total_spend\nFROM \"LiteLLM_SpendLogs\" sl\nWHERE\n  sl.\"startTime\" >= $__timeFrom()\n  AND sl.\"startTime\" < $__timeTo()\n  AND (sl.metadata->>'user_api_key_team_alias' ~ '$team_filter' OR '$team_filter' = '.*')\n  AND sl.metadata->>'user_api_key_team_alias' IS NOT NULL\n  AND sl.metadata->>'user_api_key_team_alias' != ''"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 2,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 1000}
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "📈 Прогноз на месяц по проектам",
      "type": "table",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "table",
          "rawSql": "WITH project_projections AS (\n  SELECT\n    COALESCE(sl.metadata->>'user_api_key_team_alias', 'unknown') as project,\n    ROUND((SUM(sl.spend) / NULLIF(COUNT(DISTINCT DATE(sl.\"startTime\")), 0) * 30)::numeric, 2) as projection,\n    1 as sort_order\n  FROM \"LiteLLM_SpendLogs\" sl\n  WHERE\n    sl.\"startTime\" >= $__timeFrom()\n    AND sl.\"startTime\" < $__timeTo()\n    AND (sl.metadata->>'user_api_key_team_alias' ~ '$team_filter' OR '$team_filter' = '.*')\n    AND sl.metadata->>'user_api_key_team_alias' IS NOT NULL\n    AND sl.metadata->>'user_api_key_team_alias' != ''\n  GROUP BY sl.metadata->>'user_api_key_team_alias'\n  HAVING SUM(sl.spend) > 0\n),\ntotal_projection AS (\n  SELECT\n    '🔹 ИТОГО' as project,\n    ROUND(SUM(projection)::numeric, 2) as projection,\n    0 as sort_order\n  FROM project_projections\n)\nSELECT\n  project as \"🏢 Проект\",\n  projection as \"📈 Прогноз/мес ($)\"\nFROM (\n  SELECT * FROM total_projection\n  UNION ALL\n  SELECT * FROM project_projections\n) combined\nORDER BY sort_order, projection DESC"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "left",
            "filterable": false
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "📈 Прогноз/мес ($)"},
            "properties": [
              {"id": "unit", "value": "currencyUSD"},
              {"id": "decimals", "value": 2},
              {"id": "custom.cellOptions", "value": {"type": "color-background"}},
              {"id": "color", "value": {"mode": "continuous-GrYlRd"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "🏢 Проект"},
            "properties": [
              {"id": "custom.width", "value": 180}
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{"desc": true, "displayName": "📈 Прогноз/мес ($)"}]
      }
    },
    {
      "id": 4,
      "title": "💰 Распределение расходов",
      "type": "piechart",
      "gridPos": {"h": 10, "w": 12, "x": 0, "y": 8},
      "datasource": {
        "type": "postgres",
        "uid": "postgres"
      },
      "targets": [
        {
          "refId": "A",
          "format": "time_series",
          "rawSql": "SELECT\n  NOW() as time,\n  COALESCE(sl.metadata->>'user_api_key_team_alias', 'unknown') as metric,\n  SUM(sl.spend) as value\nFROM \"LiteLLM_SpendLogs\" sl\nWHERE\n  sl.\"startTime\" >= $__timeFrom()\n  AND sl.\"startTime\" < $__timeTo()\n  AND (sl.metadata->>'user_api_key_team_alias' ~ '$team_filter' OR '$team_filter' = '.*')\n  AND sl.metadata->>'user_api_key_team_alias' IS NOT NULL\n  AND sl.metadata->>'user_api_key_team_alias' != ''\nGROUP BY sl.metadata->>'user_api_key_team_alias'\nHAVING SUM(sl.spend) > 0\nORDER BY value DESC\nLIMIT 10"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
