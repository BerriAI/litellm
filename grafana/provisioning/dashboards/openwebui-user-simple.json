{
  "id": null,
  "uid": "openwebui-user-simple",
  "title": "OpenWebUI - User Details (Simple)",
  "tags": ["litellm", "openwebui", "user-details"],
  "timezone": "browser",
  "refresh": "30s",
  "schemaVersion": 30,
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "selected_user",
        "label": "Selected User",
        "type": "query",
        "query": "label_values(litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias!=\"anonymous\"}, end_user_alias)",
        "refresh": 1,
        "multi": false,
        "includeAll": false,
        "current": {"text": "", "value": ""},
        "options": [],
        "datasource": "Prometheus"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "üë§ USER OVERVIEW - ${selected_user}",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
      "collapsed": false
    },
    {
      "id": 2,
      "title": "Total User Spending",
      "type": "stat",
      "gridPos": {"h": 6, "w": 4, "x": 0, "y": 1},
      "targets": [
        {
          "expr": "sum(increase(litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[$__range]))",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 4,
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 10}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "User Requests",
      "type": "stat",
      "gridPos": {"h": 6, "w": 4, "x": 4, "y": 1},
      "targets": [
        {
          "expr": "sum(increase(litellm_requests_total{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[$__range]))",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 4,
      "title": "Average Cost per Request",
      "type": "stat",
      "gridPos": {"h": 6, "w": 4, "x": 8, "y": 1},
      "targets": [
        {
          "expr": "sum(increase(litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[$__range])) / sum(increase(litellm_requests_total{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[$__range]))",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "decimals": 6
        }
      }
    },
    {
      "id": 5,
      "title": "Total Tokens Used",
      "type": "stat",
      "gridPos": {"h": 6, "w": 4, "x": 12, "y": 1},
      "targets": [
        {
          "expr": "sum(increase(litellm_tokens_total{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\", token_type=\"total\"}[$__range]))",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        }
      }
    },
    {
      "id": 6,
      "title": "Most Used Model",
      "type": "stat",
      "gridPos": {"h": 6, "w": 4, "x": 16, "y": 1},
      "targets": [
        {
          "expr": "topk(1, sum by (model) (litellm_requests_total{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}))",
          "format": "time_series",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "noValue": "No data"
        }
      }
    },
    {
      "id": 7,
      "title": "User Rank (by spending)",
      "type": "stat",
      "gridPos": {"h": 6, "w": 4, "x": 20, "y": 1},
      "targets": [
        {
          "expr": "count(sum by (end_user_alias) (litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias!=\"anonymous\"}) > sum(litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"})) + 1",
          "format": "time_series",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "displayName": "Rank"
        }
      }
    },
    {
      "id": 8,
      "title": "üìä MODEL USAGE BREAKDOWN",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 7},
      "collapsed": false
    },
    {
      "id": 9,
      "title": "Spending by Model",
      "type": "table",
      "gridPos": {"h": 10, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum by (model) (increase(litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[$__range]))",
          "format": "table",
          "refId": "A"
        },
        {
          "expr": "sum by (model) (increase(litellm_requests_total{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[$__range]))",
          "format": "table",
          "refId": "B"
        },
        {
          "expr": "sum by (model) (increase(litellm_tokens_total{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\", token_type=\"total\"}[$__range]))",
          "format": "table",
          "refId": "C"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {"Time": true},
            "renameByName": {
              "Value #A": "Spending",
              "Value #B": "Requests",
              "Value #C": "Tokens",
              "model": "Model"
            }
          }
        }
      ],
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "Model"},
            "properties": [{"id": "custom.width", "value": 200}]
          },
          {
            "matcher": {"id": "byName", "options": "Spending"},
            "properties": [
              {"id": "unit", "value": "currencyUSD"},
              {"id": "decimals", "value": 4}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Requests"},
            "properties": [
              {"id": "unit", "value": "short"}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "Tokens"},
            "properties": [
              {"id": "unit", "value": "short"}
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [{"desc": true, "displayName": "Spending"}]
      }
    },
    {
      "id": 10,
      "title": "Model Usage Distribution",
      "type": "piechart",
      "gridPos": {"h": 10, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "sum by (model) (increase(litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[$__range]))",
          "format": "time_series",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ],
      "options": {
        "pieType": "donut",
        "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]}
      },
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "color": {"mode": "palette-classic"}
        }
      }
    },
    {
      "id": 11,
      "title": "üìà USAGE TRENDS",
      "type": "row",
      "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
      "collapsed": false
    },
    {
      "id": 12,
      "title": "User Spending Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 19},
      "targets": [
        {
          "expr": "sum(increase(litellm_spend_usd{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\"}[1h]))",
          "format": "time_series",
          "legendFormat": "Hourly Spending",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "currencyUSD",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "pointSize": 5
          }
        }
      }
    },
    {
      "id": 13,
      "title": "Token Usage Over Time",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 19},
      "targets": [
        {
          "expr": "sum by (token_type) (increase(litellm_tokens_total{team_alias=\"OpenWebUI\", end_user_alias=\"$selected_user\", token_type!=\"total\"}[$__range]))",
          "format": "time_series",
          "legendFormat": "{{token_type}} tokens",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    }
  ],
  "links": [
    {
      "title": "‚Üê Back to Main Dashboard",
      "url": "/d/openwebui-simple",
      "targetBlank": false,
      "icon": "arrow left"
    }
  ],
  "overwrite": true
}