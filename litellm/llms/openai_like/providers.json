    "param_mappings": {
      "max_completion_tokens": "max_tokens"
    }
  },
  "poe": {
    "base_url": "https://api.poe.com/v1",
    "api_key_env": "POE_API_KEY",
    "param_mappings": {
      "max_completion_tokens": "max_tokens"
    }
  },
  "chutes": {
    "base_url": "https://llm.chutes.ai/v1/",
