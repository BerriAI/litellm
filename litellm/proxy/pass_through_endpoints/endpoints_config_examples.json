{
  "_comment": "Example configurations for common provider patterns",
  "_note": "These are examples. To use, copy to endpoints_config.json and customize",
  
  "cohere": {
    "route_prefix": "/cohere/{endpoint:path}",
    "target_base_url": "https://api.cohere.com",
    "target_base_url_env": "COHERE_API_BASE",
    "auth": {
      "type": "bearer_token",
      "env_var": "COHERE_API_KEY"
    },
    "streaming": {
      "detection_method": "url_contains",
      "pattern": "stream"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["Cohere Pass-through", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/pass_through/cohere"
  },

  "mistral": {
    "route_prefix": "/mistral/{endpoint:path}",
    "target_base_url": "https://api.mistral.ai",
    "target_base_url_env": "MISTRAL_API_BASE",
    "auth": {
      "type": "bearer_token",
      "env_var": "MISTRAL_API_KEY"
    },
    "streaming": {
      "detection_method": "request_body_field",
      "field_name": "stream"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["Mistral Pass-through", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/pass_through/mistral"
  },

  "anthropic": {
    "route_prefix": "/anthropic/{endpoint:path}",
    "target_base_url": "https://api.anthropic.com",
    "target_base_url_env": "ANTHROPIC_API_BASE",
    "auth": {
      "type": "custom_header",
      "env_var": "ANTHROPIC_API_KEY",
      "header_name": "x-api-key",
      "header_format": "{api_key}"
    },
    "streaming": {
      "detection_method": "request_body_field",
      "field_name": "stream"
    },
    "features": {
      "forward_headers": true,
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["Anthropic Pass-through", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/pass_through/anthropic_completion"
  },

  "gemini": {
    "route_prefix": "/gemini/{endpoint:path}",
    "target_base_url": "https://generativelanguage.googleapis.com",
    "target_base_url_env": "GEMINI_API_BASE",
    "auth": {
      "type": "query_param",
      "env_var": "GEMINI_API_KEY",
      "param_name": "key"
    },
    "auth_extraction": {
      "from_query_param": "key",
      "from_header": "x-goog-api-key"
    },
    "streaming": {
      "detection_method": "url_contains",
      "pattern": "stream"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true,
      "custom_query_params": true
    },
    "tags": ["Google AI Studio Pass-through", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/pass_through/google_ai_studio"
  },

  "openai": {
    "route_prefix": "/openai/{endpoint:path}",
    "target_base_url": "https://api.openai.com/",
    "target_base_url_env": "OPENAI_API_BASE",
    "auth": {
      "type": "bearer_token",
      "env_var": "OPENAI_API_KEY"
    },
    "streaming": {
      "detection_method": "url_contains",
      "pattern": "stream"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["OpenAI Pass-through", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/pass_through/openai"
  },

  "assemblyai": {
    "route_prefix": "/assemblyai/{endpoint:path}",
    "target_base_url": "https://api.assemblyai.com",
    "target_base_url_env": "ASSEMBLYAI_API_BASE",
    "auth": {
      "type": "custom_header",
      "env_var": "ASSEMBLYAI_API_KEY",
      "header_name": "Authorization",
      "header_format": "{api_key}"
    },
    "streaming": {
      "detection_method": "request_body_field",
      "field_name": "stream"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["AssemblyAI Pass-through", "pass-through"],
    "docs_url": "https://api.assemblyai.com"
  },

  "vllm": {
    "route_prefix": "/vllm/{endpoint:path}",
    "target_base_url": "http://localhost:8000",
    "target_base_url_env": "VLLM_API_BASE",
    "auth": {
      "type": "bearer_token",
      "env_var": "VLLM_API_KEY"
    },
    "streaming": {
      "detection_method": "request_body_field",
      "field_name": "stream"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["VLLM Pass-through", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/pass_through/vllm"
  },

  "custom_openai_compatible": {
    "_comment": "Template for any OpenAI-compatible API",
    "route_prefix": "/custom/{endpoint:path}",
    "target_base_url": "https://your-api.example.com/v1",
    "target_base_url_env": "CUSTOM_API_BASE",
    "auth": {
      "type": "bearer_token",
      "env_var": "CUSTOM_API_KEY"
    },
    "streaming": {
      "detection_method": "request_body_field",
      "field_name": "stream"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["Custom Provider Pass-through", "pass-through"],
    "docs_url": "https://your-api.example.com/docs"
  }
}
