{
  "_comment": "JSON-based endpoint configurations for LiteLLM pass-through endpoints",
  "_version": "1.0.0",
  "_schema_url": "https://docs.litellm.ai/docs/pass_through/json_config_schema",
  
  "google_imagen": {
    "_description": "Google Imagen API via Google AI Studio - Simple API key auth for image generation",
    "route_prefix": "/google_imagen/{endpoint:path}",
    "target_base_url": "https://generativelanguage.googleapis.com/v1beta",
    "target_base_url_env": "GOOGLE_IMAGEN_API_BASE",
    "auth": {
      "type": "query_param",
      "env_var": "GOOGLE_API_KEY",
      "param_name": "key"
    },
    "streaming": {
      "detection_method": "none"
    },
    "features": {
      "require_litellm_auth": true,
      "subpath_routing": true,
      "custom_query_params": true,
      "forward_headers": false
    },
    "tags": ["Google Imagen Pass-through", "Image Generation", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/providers/vertex_image"
  },

  "example_provider": {
    "route_prefix": "/example/{endpoint:path}",
    "target_base_url": "https://api.example.com",
    "target_base_url_env": "EXAMPLE_API_BASE",
    "auth": {
      "type": "bearer_token",
      "env_var": "EXAMPLE_API_KEY",
      "header_format": "Bearer {api_key}"
    },
    "streaming": {
      "detection_method": "request_body_field",
      "field_name": "stream"
    },
    "features": {
      "forward_headers": false,
      "merge_query_params": false,
      "require_litellm_auth": true,
      "subpath_routing": true
    },
    "tags": ["Example Provider Pass-through", "pass-through"],
    "docs_url": "https://docs.litellm.ai/docs/pass_through/example"
  }
}
