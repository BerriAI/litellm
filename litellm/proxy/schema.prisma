datasource client {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-py"
}

// required for token gen
model LiteLLM_VerificationToken {
    token      String   @unique
    spend      Float    @default(0.0)
    expires    DateTime
    models     String[]
    aliases    Json  @default("{}")
    config     Json  @default("{}")
}